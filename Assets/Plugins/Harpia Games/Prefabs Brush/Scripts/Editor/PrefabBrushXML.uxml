    <ui:VisualElement name="status-section" style="flex-grow: 1; background-color: rgba(255, 0, 0, 0.39); position: absolute; flex-direction: column; top: 0; left: 0; align-items: stretch; justify-content: space-around; width: 100%; padding-left: 10px; padding-right: 10px; padding-top: 10px; padding-bottom: 10px;">
