                <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row-reverse;">
                    <ui:Button text="Save Template" display-tooltip-when-elided="true" name="save-template" style="width: 120px;" />
                </ui:VisualElement>
