            <ui:Foldout text="&lt;b&gt;Main Settings&lt;/b&gt;" name="main-settings-foldout" style="margin-bottom: 10px; margin-top: 10px; transition-duration: 0.5s; transition-property: visibility;">
                <uie:EnumField label="Use Mode" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" name="mode-dropdown" focusable="false" tooltip="Precision Mode: Place one prefab at a time with each click.&#10;&#10;Multiple Mode: Place multiple prefabs within the brush area with each click." />
                <uie:EnumField label="Raycast Mode" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" name="raycast-mode" tooltip="Mesh Mode: Prefabs will be placed according to the shape of the mesh surface&#10;&#10;Physical Collider Mode: Prefabs will be placed based on the boundaries of the collider.&#10;&#10;Attention: Terrains only work with Physical collider mode" />
            </ui:Foldout>
            <ui:Foldout text="&lt;b&gt;Precision Mode&lt;/b&gt;" name="single-section" style="margin-bottom: 10px;">
                <ui:Toggle label="Change Prefab after placing  " name="single-mode-change-prefab" tooltip="The selected prefab will change after placing the object. This can be useful for creating nature environments." />
                <ui:Toggle label="Add Painted to Raycast Mesh" name="single-mode-add-mesh" tooltip="Add Painted to Raycast Mesh: If checked, newly placed prefabs will be included in the mesh used for raycast placements. This allows subsequently placed prefabs to be positioned on top of previously placed ones." />
                <uie:FloatField label="Shortcut Rotation Angle" value="15" tooltip="Shortcut Rotation Angle: Specify the rotation angle (in degrees) applied to the prefab when using the rotation shortcut key. This value determines how much the prefab will rotate with each press of the shortcut." name="shortcut-rotation-angle" />
            </ui:Foldout>
            <ui:Foldout text="&lt;b&gt;Parenting&lt;/b&gt;" name="parent-section" style="-unity-font-style: normal; margin-bottom: 10px;">
                <uie:EnumField label="Parent Mode" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" name="parenting-mode" tooltip="No Parent: Placed prefabs will not have a parent object and will be independent in the hierarchy.&#10;&#10;Fixed Parent: All prefabs placed will be parented to a specified object.&#10;&#10;Hit Surface Parent: Prefabs will be parented to the object they are placed on. Please note that the parent scale and rotation can change the new object transforms." />
                <ui:Label tabindex="-1" text="⚠ Required Parent" display-tooltip-when-elided="true" name="required-parent-label" style="background-color: rgba(255, 0, 0, 0.39); padding-left: 5px; padding-right: 5px; padding-top: 5px; padding-bottom: 5px; margin-top: 10px; border-top-left-radius: 5px; border-bottom-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px;" />
                <uie:ObjectField label="Parent" name="parent" allow-scene-objects="true" type="UnityEngine.Transform, UnityEngine.CoreModule" tooltip="Parent: Assign a transform here to set it as the parent for the painted prefabs. This helps in organizing and grouping painted objects within the hierarchy." />
                <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row;">
                    <ui:TextField picking-mode="Ignore" label="New Parent" name="parent-name" tooltip="New Parent: Input a name to create and set a new default parent for painted prefabs." style="flex-grow: 1;" />
                    <ui:Button text="Create and use" display-tooltip-when-elided="true" name="create-parent" />
                </ui:VisualElement>
                <ui:Toggle label="Uniform Scaled Parents Only" tooltip="Enable this toggle to create prefabs only to parent objects with a uniform scale. This ensures that the object&apos;s scale remains consistent, preventing any weird or distorted appearances." value="true" name="uniform-scaled-parent-only" />
            </ui:Foldout>
            <ui:Foldout text="&lt;b&gt;Brush Multiple Mode&lt;/b&gt;" name="brush-section" style="margin-bottom: 10px;">
                <ui:Slider label="Size" value="1" high-value="10" name="size" show-input-field="true" tooltip="Brush Size: Adjust to increase or decrease the area of effect for prefab placement." />
                <ui:Slider label="Strength" value="0.3" high-value="1" name="strength" show-input-field="true" tooltip="Brush Strength: Adjust to increase or decrease the density of prefabs placed within the brush area." />
                <ui:Slider picking-mode="Ignore" label="Clipping Tolerance" low-value="0.01" value="0.5" high-value="0.99" name="clipping-strength" show-input-field="true" tooltip="Adjusts the allowed degree of mesh overlap. Higher values permit more overlapping, while lower values are stricter in preventing mesh clipping." />
            </ui:Foldout>
            <ui:Foldout text="&lt;b&gt;Placement&lt;/b&gt;" name="placement-section" value="true" style="margin-bottom: 10px;">
                <uie:LayerMaskField label="Raycast Layers" name="layer" tooltip="Select the layers on which the brush is allowed to place prefabs.">
                    <ui:Button text="Physics Debugger" display-tooltip-when-elided="true" name="physics-debbuger-btn" />
                </uie:LayerMaskField>
                <uie:MaskField label="Allowed Tags" choices="System.Collections.Generic.List`1[System.String]" name="tags" tooltip="Select the tags of objects on which the brush is allowed to place prefabs." style="margin-bottom: 10px;" />
                <uie:EnumField label="Snap Mode" value="Center" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" name="snap-mode-dropdown" tooltip="Define how objects snap in the scene – either to the grid or to mesh vertices of other objects." />
                <ui:VisualElement name="grid-snapping-section" style="flex-grow: 1; margin-bottom: 10px;">
                    <ui:VisualElement name="grid-snap-section" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0);">
                        <uie:FloatField label="Grid Size" value="1" name="grid-snap-value" is-delayed="true" tooltip="Snap Size: Set the grid&apos;s spacing for object placement when Grid Snapping is enabled. Increase for wider spacing, or decrease for tighter alignment." />
                        <uie:Vector2Field label="Grid Offset" name="grid-offset" tooltip="Grid Offset: Control the grid&apos;s starting point for better synchronization with existing scene structures." value="0,-0.03" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="vertex-snapping-section" style="flex-grow: 1; margin-bottom: 0;">
                    <uie:LayerMaskField label="Vertex Snap Layer Mask" name="vertex-snapping-layer-mask" tooltip="Define the layers that will be utilized when snapping to vertices." value="-1" />
                    <uie:MaskField label="Vertex Snap Tag Mask" name="vertex-snapping-tag-mask" tooltip="Define the object tags that will be allowed for vertex snapping" value="-1" />
                    <uie:FloatField label="Vertex Snap Distance" value="1" name="vertex-snapping-snap-distance" tooltip="Specify the closest distance at which vertex snapping becomes active." />
                </ui:VisualElement>
                <ui:VisualElement name="texture-mask-section" style="flex-grow: 1; margin-bottom: 10px;">
                    <ui:Toggle label="Use Texture Mask" name="use-texture-mask-toggle" tooltip="Texture Mask: Enable to paint objects on terrain areas matching the selected texture." style="margin-top: 10px;" />
                    <ui:VisualElement name="allowed-textures-section" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row;">
                        <ui:Label tabindex="-1" text="Allowed Textures" display-tooltip-when-elided="true" tooltip="Allowed Textures: Specify which terrain textures permit object painting. Objects will only be painted on these designated areas." style="width: 140px; -unity-text-align: middle-left; padding-left: 4px;" />
                        <ui:ScrollView mode="Horizontal" name="allowed-textures-holder">
                            <ui:VisualElement style="flex-grow: 1; background-color: rgb(255, 0, 0); height: 20px; width: 20px; margin-right: 2px; margin-left: 2px;" />
                        </ui:ScrollView>
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="angles-section" style="flex-grow: 1; border-bottom-width: 10px;">
                    <ui:Toggle label="Use Angle Limits" name="use-angle-limits" tooltip="Use Angle Limits: If checked, you can specify a range of angles at which the prefab can be placed. Helps to prevent placement on steep slopes" />
                    <ui:VisualElement name="min-max-angles">
                        <uie:FloatField value="0" name="min-slope" label="Min Angle" tooltip="Min Angle: Set the minimum allowable angle (in degrees) between the surface normal and the up direction for prefab placement." />
                        <uie:FloatField value="180" name="max-slope" label="Max angle" tooltip="Max Angle: Set the maximum allowable angle (in degrees) between the surface normal and the up direction for prefab placement." />
                    </ui:VisualElement>
                    <ui:VisualElement name="angles-slider" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row; padding-left: 2px; padding-right: 2px;">
                        <ui:Label tabindex="-1" text="Min " display-tooltip-when-elided="false" picking-mode="Ignore" enable-rich-text="false" style="-unity-text-align: lower-left;" />
                        <ui:MinMaxSlider low-limit="0" high-limit="180" name="slope-limits" value="0,90" style="flex-grow: 1;" />
                        <ui:Label tabindex="-1" text="Max" display-tooltip-when-elided="false" picking-mode="Ignore" enable-rich-text="false" style="-unity-text-align: lower-left;" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="fake-plane-section" style="flex-grow: 1;">
                    <ui:Toggle label="No Raycast Use Fake Plane" name="no-hit-y-pos-toggle" tooltip="If there&apos;s no raycast hit the tool will use a Imaginary Plane  to raycast the hit. This is useful for placing Tiles. " style="justify-content: flex-start; align-items: center;" />
                    <uie:FloatField label="Fake Plane Y Position" value="42.2" tooltip="The Y Position if no hit" name="fake-plane-y-pos" />
                </ui:VisualElement>
            </ui:Foldout>
            <ui:Foldout text="&lt;b&gt;Transforms&lt;/b&gt;" name="transform-section" value="true" style="margin-bottom: 10px;">
                <uie:EnumField label="Pivot Mode" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" name="pivot-mode" tooltip="Mesh Pivot: The prefab will be placed with its original pivot point at the placement point.&#10;&#10;Bounds Center: The prefab will be placed with the center of its bounding box at the placement point." />
                <ui:VisualElement name="custom-pivot-selection" style="flex-grow: 1; flex-direction: row;">
                    <ui:Label tabindex="-1" text="1/30" parse-escape-sequences="true" display-tooltip-when-elided="true" name="custom-pivot-selection-number" style="-unity-text-align: upper-center; min-width: 58px;" />
                    <ui:Label tabindex="-1" text="Label" parse-escape-sequences="true" display-tooltip-when-elided="true" name="custom-pivot-selection-name" style="-unity-text-align: upper-left; flex-grow: 1;" />
                    <ui:Button text="Previous" parse-escape-sequences="true" display-tooltip-when-elided="true" name="custom-pivot-selection-previous" />
                    <ui:Button text="Next" parse-escape-sequences="true" display-tooltip-when-elided="true" name="custom-pivot-selection-next" />
                </ui:VisualElement>
                <ui:Toggle label="Align to Hit" name="align-with-ground" tooltip="Align to Hit: If checked, the prefab will align with the surface normal of the hit point." style="margin-bottom: 5px; margin-top: 5px;" />
                <ui:VisualElement picking-mode="Ignore" style="flex-grow: 1; flex-direction: row; padding-left: 3px;">
                    <ui:Label text="Align To Hit Axis" style="margin-bottom: 5px; margin-right: 40px;" />
                    <ui:VisualElement style="flex-grow: 1; flex-direction: row;">
                        <ui:Toggle text="X" name="align-to-hit-axis-x" value="true" />
                        <ui:Toggle text="Y" name="align-to-hit-axis-y" value="true" />
                        <ui:Toggle text="Z" name="align-to-hit-axis-z" value="true" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="rotation-transform" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); min-height: 70px; margin-bottom: 5px;">
                    <uie:EnumField label="Rotation Type" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" value="Center" name="rotation-type" tooltip="Random: for varied object orientations&#10;&#10;Fixed: for a consistent rotation angle during placement.&#10;&#10;Scene Camera Rotation:aligns the rotation of new objects with the scene&apos;s camera orientation." />
                    <uie:Vector3Field label="Fixed Rotation" name="rotation-fixed" />
                    <uie:Vector3Field label="Max Rotation" name="rotation-max" value="0,360,0" />
                    <uie:Vector3Field label="Min Rotation" name="rotation-min" />
                    <ui:VisualElement name="rotation-look-at-section" style="flex-grow: 1;">
                        <ui:VisualElement style="flex-grow: 1; flex-direction: row;">
                            <ui:Label text="Look at axis:" picking-mode="Ignore" style="width: 155px;" />
                            <ui:Toggle text=" X" name="rotation-look-at-axis-x" />
                            <ui:Toggle text="Y" name="rotation-look-at-axis-y" />
                            <ui:Toggle text="Z" name="rotation-look-at-axis-z" />
                        </ui:VisualElement>
                        <uie:ObjectField label="Look At Transform" type="UnityEngine.Transform, UnityEngine.CoreModule" name="rotation-look-at-transform" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="scale-transform" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); min-height: 70px; margin-bottom: 5px;">
                    <uie:EnumField label="Scale Type" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" value="Center" name="scale-type" tooltip="Scale Type: Select &apos;Random&apos; for varied object sizes or &apos;Fixed&apos; for a consistent scale during placement." />
                    <uie:Vector3Field label="Fixed Scale" name="scale-fixed" value="1,1,1">
                        <ui:VisualElement name="scale-proportions-toggle-fixed" style="background-color: rgba(0, 0, 0, 0); width: 20px;" />
                    </uie:Vector3Field>
                    <uie:Vector3Field label="Max Scale" name="scale-max">
                        <ui:VisualElement name="scale-proportions-toggle-max" tooltip="enable constraint" style="background-color: rgba(0, 0, 0, 0); width: 20px;" />
                    </uie:Vector3Field>
                    <uie:Vector3Field label="Min Scale" name="scale-min">
                        <ui:VisualElement name="scale-proportions-toggle-min" style="background-color: rgba(0, 0, 0, 0); width: 20px;" />
                    </uie:Vector3Field>
                    <uie:Vector3Field label="Max Scale Uniform" name="scale-max-uniform" value="1,1,1" />
                    <uie:Vector3Field label="Min Scale Uniform" name="scale-min-uniform" />
                </ui:VisualElement>
                <ui:VisualElement name="offset-transform" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); min-height: 70px;">
                    <uie:EnumField label="Offset Type" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" value="Center" name="offset-type" tooltip="Offset Type: Choose &apos;Random&apos; for unpredictable object positioning offsets or &apos;Fixed&apos; for a uniform offset during placement." />
                    <uie:Vector3Field label="Fixed Offset" name="offset-fixed" />
                    <uie:Vector3Field label="Max Offset" name="offset-max" value="0,0.5,0" />
                    <uie:Vector3Field label="Min Offset" name="offset-min" value="0,0.1,0" />
                </ui:VisualElement>
            </ui:Foldout>
            <ui:Foldout text="&lt;b&gt;Physics&lt;/b&gt;" name="physics-secttion" style="margin-bottom: 10px;">
                <ui:Label tabindex="-1" text="⚠ No Physical Complatible GameObject In Your List" display-tooltip-when-elided="true" name="no-rigidbody-selected" tooltip="We could not find any object with a Non Kinematic Rigidbody and a Non Trigger Collider in your obejcts list" style="background-color: rgba(255, 236, 0, 0.11); padding-left: 5px; padding-right: 5px; padding-top: 5px; padding-bottom: 5px; margin-top: 0; border-top-left-radius: 5px; border-bottom-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px; margin-bottom: 5px;" />
                <ui:Toggle value="false" text=" Simulate Physics" display-tooltip-when-elided="true" name="simulate-physics-toggle" tooltip="Simulate Physics: Toggle to activate real-time physics interactions for painted objects. When off, physics will remain static." style="margin-bottom: 10px;" />
                <uie:EnumField label="Simulation Mode" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" value="Center" name="physics-simulation-mode" tooltip="Physics Simulation Mode: Choose &apos;Automatic&apos; for continuous physics interactions or &apos;Step&apos; for controlled, incremental simulations.">
                    <ui:Button text="Physics Settings" display-tooltip-when-elided="true" enable-rich-text="false" name="btn-physics-settings" />
                </uie:EnumField>
                <uie:EnumField label="Affect Objects" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" value="Center" name="physics-affect-objects" tooltip="Affect Objects: Choose the objects affected by operations. &apos;All&apos; targets every object, &apos;Only New Spawned&apos; affects recently added objects, and &apos;Only Selected&apos; operates on your current selections." />
                <ui:VisualElement name="physics-step-section" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row;">
                    <ui:Slider picking-mode="Ignore" label="Step Value" value="0.02" high-value="0.15" low-value="0.01" name="physics-step-slider" show-input-field="true" tooltip="Step Value: Adjust the increment for step-by-step physics simulations. A higher value results in larger simulation jumps per step." style="margin-bottom: 10px; flex-grow: 1;" />
                    <ui:Button text="Step Physics" display-tooltip-when-elided="true" name="step-physics-button" />
                </ui:VisualElement>
