            <ui:VisualElement name="prefabs-section" class="section" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); margin-top: 15px; padding-left: 4px;">
                <ui:VisualElement name="prefab-buttons" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); align-items: center; flex-direction: row; justify-content: space-between;">
                    <ui:Label tabindex="-1" text="Prefabs" display-tooltip-when-elided="true" picking-mode="Ignore" class="h1" style="font-size: 14px; -unity-font-style: bold;" />
                    <ui:Button text="Clear List" display-tooltip-when-elided="true" name="clear-list" tooltip="Clear Prefab List" style="width: 120px;" />
                </ui:VisualElement>
