            <ui:Button text="⌫ &lt;b&gt;Erase Mode&lt;/b&gt;" display-tooltip-when-elided="true" name="erase-mode-button" tooltip="Shortcut: B" style="font-size: 12px; width: 130px; height: 20px;" />
            <ui:Button text="✷ &lt;b&gt;Settings&lt;/b&gt;" display-tooltip-when-elided="true" name="settings-button" style="font-size: 12px; width: 130px; height: 20px;" />
            <ui:Button text="☰ &lt;b&gt;Documentation&lt;/b&gt;" display-tooltip-when-elided="true" name="documentation-button" style="font-size: 12px; width: 130px; height: 20px;" />
        </ui:VisualElement>
        <ui:Foldout name="erase-mode" text="&lt;b&gt;Erase&lt;/b&gt;" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); margin-top: 10px; margin-bottom: 10px;">
            <uie:LayerMaskField label="Eraser Layer Mask" name="erase-mask" value="-1" tooltip="Will only delete objects that are inside this layer mask" />
            <uie:MaskField label="Eraser Tag Mask" name="eraser-tag-mask" value="1" />
            <ui:Slider picking-mode="Ignore" label="Eraser Radius" value="2" high-value="15" name="eraser-radius-slider" low-value="0.1" show-input-field="true" tooltip="Eraser Radius: Adjusts the size of the area where prefabs will be removed. Increase for larger deletion areas, or decrease for more precise erasing." />
